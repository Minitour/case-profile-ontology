  <owl:Ontology rdf:about="">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >K4Care Actor Profile Ontology</rdfs:comment>
    <co-author rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
