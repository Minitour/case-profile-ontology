    <rdfs:subClassOf>
      <owl:Class rdf:ID="InterventionPlanDefinitionService"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#serviceInitiatedBy"/>
        </owl:onProperty>
